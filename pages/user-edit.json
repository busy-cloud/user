{
  "title": "编辑用户",
  "template": "form",
  "fields": [
    {
      "key": "id",
      "label": "ID",
      "type": "text",
      "required": true
    },
    {
      "key": "name",
      "label": "名称",
      "type": "text",
      "required": true
    },
    {
      "key": "email",
      "label": "邮箱",
      "type": "text"
    },
    {
      "key": "cellphone",
      "label": "手机",
      "type": "text"
    },
    {
      "key": "admin",
      "label": "管理员",
      "type": "switch"
    },
    {
      "key": "disabled",
      "label": "禁用",
      "type": "switch"
    }
  ],
  "load_api": "table/user/:id",
  "submit_api": "table/user/:id",
  "submit_success": "this.navigate('/page/user/user')",
  "methods": {
  }
}